	<queryString><![CDATA[select d.name as warehouse, b.name as product, sum(a.movementqty) as qtyonhand, e.cost, sum(a.movementqty*e.cost) as inventoryvalue 
from m_transaction a
inner join m_product b on b.m_product_id=a.m_product_id
inner join m_locator c on c.m_locator_id=a.m_locator_id
inner join m_warehouse d on d.m_warehouse_id=c.m_warehouse_id
left join m_costing e on e.m_product_id=a.m_product_id and e.datefrom<=$P{tdate} and e.dateto>=$P{tdate}
						 and (e.m_warehouse_id=d.m_warehouse_id or e.m_warehouse_id is null)
where a.movementdate<=$P{tdate}
and d.m_warehouse_id=$P{m_warehouse_id}
and (case when $P{m_product_category_id} is not null then b.m_product_category_id=$P{m_product_category_id} else 1=1 end)
group by d."name", b."name", e.m_costing_id
order by d."name", b."name"]]></queryString>

