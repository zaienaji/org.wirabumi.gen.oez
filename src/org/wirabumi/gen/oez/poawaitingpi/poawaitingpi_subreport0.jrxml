	<queryString>
		<![CDATA[select distinct ol.line, (select name from m_product where m_product_id=ol.m_product_id) as productname,
	qtyordered, COALESCE((select sum(il2.qtyinvoiced) from c_invoiceline il2 where il2.c_orderline_id=il.c_orderline_id),0) as qtydelivered, (qtyordered-COALESCE((select sum(il2.qtyinvoiced) from c_invoiceline il2 where il2.c_orderline_id=il.c_orderline_id),0)) as qtyoutstanding
from c_orderline ol
left join c_invoiceline il on il.c_orderline_id=ol.c_orderline_id
where ol.c_order_id=$P{p_order_id}
and qtyordered>COALESCE((select sum(il2.qtyinvoiced) from c_invoiceline il2 where il2.c_orderline_id=il.c_orderline_id),0)]]>
	</queryString>
